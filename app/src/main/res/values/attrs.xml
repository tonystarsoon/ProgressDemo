<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="RingProgress">
        <attr name="backgroundColor" format="color" />
        <attr name="ringBackground" format="color" />
        <attr name="progressColor" format="color" />
        <attr name="ringStroke" format="dimension" />
        <attr name="alphaProgressColor" format="boolean" />
        <attr name="maxValue" format="integer" />
        <attr name="footerTextSize" format="dimension" />
        <attr name="centerTextSize" format="dimension" />
        <attr name="progressType" format="enum">
            <enum name="percent" value="0" />
            <enum name="values" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="LinearProgress">
        <attr name="linearProgressBackground" format="color" />
        <attr name="linearProgressBackgroundColor" format="color" />
        <attr name="linearProgressColor" format="color" />
        <attr name="linearProgressTextSize" format="dimension" />
        <attr name="linearProgressMaxValue" format="integer" />
        <attr name="linearProgressRadius" format="dimension" />
        <attr name="linearProgressDrawText" format="boolean" />
        <attr name="linearProgressTextColor" format="color" />
        <attr name="linearProgressOrientational" format="enum">
            <enum name="horizontal" value="0" />
            <enum name="vertical_down" value="1" />
            <enum name="vertical_up" value="2" />
        </attr>
        <attr name="linearProgressType" format="enum">
            <enum name="percent" value="0" />
            <enum name="values" value="1" />
        </attr>
    </declare-styleable>

    <declare-styleable name="MyRadioGroup">
        <attr name="lineCount" format="integer" />
    </declare-styleable>
</resources>




